{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cosmos": {
    "ConnString": "<cosmosConnectionString>",
    "Containers": [
      {
        "DatabaseId": "<databaseName>",
        "ContainerId": "Todo",
        "PkInfo": {
          "PartitionKey": "/listId",
          "Template": "{0}",
          "Pattern": "^-?\\d+$"
        }
      },
      {
        "DatabaseId": "<databaseName>",
        "ContainerId": "Post",
        "PkInfo": {
          "PartitionKey": "/partitionKey",
          "Template": "{0}:{1}",
          "Pattern": "^\\d+:\\d+$"
        }
      }
    ]
  }
}
